      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 37.444, lng: -122.162984},
          zoom: 20
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBmRdwxGY1DPN5COmq5iV7ComlYMQ-2Xcs&callback=initMap"
    async defer></script>
